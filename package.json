{
  "name": "pomodoro-next",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "e2e:run": "playwright test",
    "fix": "pnpm format && pnpm lint:fix",
    "format": "prettier --write 'src/**/*.{mjs,ts,tsx}'",
    "lint": "next lint",
    "lint:fix": "next lint --fix 'src/**/*.{mjs,ts,tsx}'",
    "prepare": "husky install",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "storybook-ci": "storybook dev -p 6006 --ci",
    "test": "pnpm ut:coverage",
    "ut:coverage": "vitest run --coverage",
    "ut:run": "vitest run",
    "ut:watch": "vitest",
    "vrt:capture": "storycap --serverCmd 'pnpm storybook-ci' http://localhost:6006",
    "vrt:capture-verbose": "pnpm vrt:capture -- --verbose",
    "vrt:compare": "reg-suit compare",
    "vrt:run": "reg-suit run"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "autoprefixer": "^10.4.15",
    "next": "^13.4.19",
    "postcss": "^8.4.29",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "unplugin-auto-import": "^0.16.6",
    "use-sound": "^2.0.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@playwright/test": "^1.37.1",
    "@storybook/addon-backgrounds": "^7.2.1",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.1",
    "@storybook/preset-create-react-app": "^7.2.1",
    "@storybook/react": "^7.2.1",
    "@storybook/react-webpack5": "^7.2.1",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.6.2",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.1",
    "eslint": "^8.0.1",
    "eslint-config-next": "^13.5.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "happy-dom": "^10.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "prettier-plugin-packagejson": "^2.4.5",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prop-types": "^15.8.1",
    "reg-keygen-git-hash-plugin": "^0.12.2",
    "reg-notify-github-plugin": "^0.12.2",
    "reg-suit": "^0.12.2",
    "storybook": "^7.2.1",
    "storycap": "^4.2.0",
    "typescript": "^5.2.2",
    "url-loader": "^4.1.1",
    "vercel": "^31.2.2",
    "vitest": "^0.34.1",
    "webpack": "^5.88.2"
  },
  "packageManager": "pnpm@8.7.1",
  "engines": {
    "node": "18.x"
  }
}
