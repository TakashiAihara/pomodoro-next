{
  "name": "pomodoro-react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "fix": "npm run format && npm run lint:fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "start": "react-scripts start",
    "storybook": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "storybook-ci": "storybook dev -p 6006 --ci",
    "test": "react-scripts test",
    "vrt:capture": "npx storycap --captureTimeout 3000 -p 8 --serverCmd 'npm run storybook-ci' http://localhost:6006",
    "vrt:capture-verbose": "npm run vrt:capture -- --verbose",
    "vrt:compare": "reg-suit compare",
    "vrt:run": "reg-suit run"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "react": "^17.0.1",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^17.0.1",
    "react-scripts": "^5.0.1",
    "use-sound": "^2.0.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@storybook/addon-backgrounds": "^7.2.1",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.1",
    "@storybook/preset-create-react-app": "^7.2.1",
    "@storybook/react": "^7.2.1",
    "@storybook/react-webpack5": "^7.2.1",
    "@storybook/testing-library": "^0.2.0",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "prettier-plugin-packagejson": "^2.4.5",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prop-types": "^15.8.1",
    "reg-keygen-git-hash-plugin": "^0.12.2",
    "reg-notify-github-plugin": "^0.12.2",
    "reg-suit": "^0.12.2",
    "storybook": "^7.2.1",
    "storycap": "^4.2.0",
    "typescript": "^4.9.5",
    "vercel": "^31.2.2",
    "webpack": "^5.88.2"
  },
  "engines": {
    "node": "16.x"
  }
}
